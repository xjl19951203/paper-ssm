<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.paper.ssm.mapper.EdgeDao">

    <resultMap id="BaseResultMap" type="com.paper.ssm.model.dataPlane.Edge">
        <id column="id" jdbcType="INTEGER" property="id" />
        <result column="title" jdbcType="VARCHAR" property="title" />
<!--        <association property="data" column="dataId" select="com.paper.ssm.mapper.DataDao.selectByPrimaryKey"/>-->
    </resultMap>

    <select id="selectByPrimaryKey" resultMap="BaseResultMap" parameterType="java.lang.Integer">
        SELECT * from `element_edge` where id = #{pk};
    </select>

    <select id="selectListByQuery" resultMap="BaseResultMap" parameterType="com.paper.ssm.model.dataPlane.Edge">
        select * from `element_edge` where graphId = #{graphId}
        AND direction = #{direction};
    </select>

</mapper>